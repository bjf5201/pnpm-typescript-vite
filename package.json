{
	"name": "github-config",
	"author": "Bethany Fannin <contact@bethanyfannin.com>",
	"version": "1.0.0",
	"description": "GitHub configuration files",
	"type": "module",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "vite",
		"test": "vitest",
		"test:cover": "vitest --coverage",
		"test:ci": "vitest run --coverage",
		"lint": "eslint . --max-warnings=0",
		"format": "prettier --write .",
		"typecheck:node": "tsc -p tsconfig.node.json --noEmit",
		"typecheck:browser": "tsc -p tsconfig.browser.json --noEmit",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"prebuild": "rimraf dist && pnpm run typecheck",
		"build": "vite build",
		"serve": "pnpm run build && vite preview"
	},
	"repository": {
		"type": "git",
		"url": "git+github.com:bjf5201/.github.git"
	},
	"bugs": {
		"url": "https://github.com/bjf5201/.github/issues"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"@app": "./src/index.tsx",
				"types": "./dist/esm/types/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"export": {
				"@app": "./src/index.tsx",
				"types": "./dist/cjs/types/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=22"
	},
	"dependencies": {
		"react": "19.0.0",
		"react-dom": "19.0.0"
	},
	"packageManager": "pnpm@10.6.4+sha512.da3d715bfd22a9a105e6e8088cfc7826699332ded60c423b14ec613a185f1602206702ff0fe4c438cb15c979081ce4cb02568e364b15174503a63c7a8e2a5f6c",
	"devDependencies": {
		"@eslint/js": "9.23.0",
		"@tailwindcss/postcss": "4.0.15",
		"@tailwindcss/vite": "4.0.15",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "16.2.0",
		"@testing-library/user-event": "14.6.1",
		"@types/node": "22.13.11",
		"@types/react": "19.0.12",
		"@types/react-dom": "19.0.4",
		"@typescript-eslint/eslint-plugin": "8.27.0",
		"@typescript-eslint/parser": "8.27.0",
		"@vitejs/plugin-react-swc": "3.8.1",
		"@vitest/coverage-v8": "3.0.9",
		"@vitest/eslint-plugin": "1.1.38",
		"autoprefixer": "10.4.21",
		"eslint": "9.23.0",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.19",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"globals": "16.0.0",
		"jsdom": "26.0.0",
		"lint-staged": "15.5.0",
		"mkdirp": "3.0.1",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"rimraf": "6.0.1",
		"tailwindcss": "4.0.15",
		"typescript": "5.8.2",
		"typescript-eslint": "8.27.0",
		"vite": "6.2.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.0.9"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@swc/core",
			"esbuild"
		]
	}
}
